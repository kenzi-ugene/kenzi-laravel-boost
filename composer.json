{
    "name": "kenzi-laravel/boost",
    "description": "Laravel Boost accelerates AI-assisted development to generate high-quality, Laravel-specific code.",
    "keywords": [
        "dev",
        "laravel",
        "ai"
    ],
    "homepage": "https://github.com/kenzi-ugene/kenzi-laravel-boost.git",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/kenzi-ugene/kenzi-laravel-boost/issues",
        "source": "https://github.com/kenzi-ugene/kenzi-laravel-boost"
    },
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/console": "^8.0|^9.0",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/routing": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "laravel/mcp": "^0.1.0",
        "laravel/prompts": "^0.1.0",
        "laravel/roster": "^0.1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1.0|^2.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Boost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Boost\\BoostServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": [
            "vendor/bin/pint",
            "vendor/bin/phpstan"
        ],
        "test": [
            "vendor/bin/pest"
        ],
        "check": [
            "@composer lint",
            "@composer test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    {
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kenzi-ugene/kenzi-laravel-boost.git"
        }
    ],
    "require": {
        "kenzi-laravel/boost": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

    "minimum-stability": "dev",
    "prefer-stable": true
}
